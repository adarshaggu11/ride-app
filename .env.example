# Environment Variables for Mana Auto Oka Ride

# Google Maps API Key (Get from: https://console.cloud.google.com/)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# WebSocket Server URL for real-time updates
VITE_WS_URL=ws://localhost:3001

# API Base URL
VITE_API_BASE_URL=http://localhost:3000/api

# Firebase Configuration (for push notifications)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Payment Gateway Keys (Razorpay/Stripe)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# SMS Gateway (Twilio/MSG91)
VITE_SMS_API_KEY=your_sms_api_key

# Other Configuration
VITE_APP_ENV=development
VITE_ENABLE_ANALYTICS=false
